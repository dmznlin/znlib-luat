{
  "enable": true,
  "host": "123.56.150.11",
  "port": 8083,
  "client_id": "$id$",
  "user_name": "user_etc",
  "password": "user_etc",
  "keep_alive": 1000,
  "re_conn": 5000,
  "subs": {
    "own": {
      "topic": "etc/sub/$id$",
      "qos": 0
    },
    "srv": {
      "topic": "etc/srv",
      "qos": 0
    }
  },
